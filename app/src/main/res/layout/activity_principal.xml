<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.christian.sifacmovil.PrincipalActivity">



    <Button
        android:id="@+id/btIngresarClientes"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:text="Consultar "
        tools:layout_editor_absoluteX="99dp"

        tools:layout_editor_absoluteY="57dp"
        android:layout_alignTop="@+id/consulta"

        android:layout_marginRight="9dp"
        android:layout_marginEnd="9dp" />

    <Button
        android:id="@+id/consulta"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:text="Consultar clientes"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="449dp"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="83dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btIngresarClientes"
        android:layout_toEndOf="@+id/btIngresarClientes" />

    <Spinner
        android:id="@+id/ListaProdoctos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
